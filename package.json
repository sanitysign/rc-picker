{
  "name": "@sanitysign/rc-picker",
  "version": "0.1.11",
  "description": "React date & time picker",
  "keywords": [
    "react",
    "react-component",
    "react-picker"
  ],
  "main": "./lib/index",
  "source": "src/index.tsx",
  "module": "./es/index",
  "files": [
    "assets/*.css",
    "assets/*.scss",
    "es",
    "lib"
  ],
  "homepage": "https://react-component.github.io/picker",
  "repository": {
    "type": "git",
    "url": "git@github.com:/sanitysign/rc-picker.git"
  },
  "license": "MIT",
  "scripts": {
    "start": "npm --prefix ./sandbox run start",
    "build": "father build && sass --no-source-map sandbox/index.scss assets/index.css && copyfiles sandbox/index.scss assets --flat",
    "docs:dev": "dumi dev",
    "docs:build": "dumi build",
    "gh-pages": "npm run build && father doc deploy",
    "lint": "eslint src/ --ext .ts,.tsx,.jsx,.js,.md",
    "prettier": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "rc-test",
    "coverage": "father test --coverage",
    "now-build": "npm run docs:build"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.1",
    "@rc-component/trigger": "^1.5.0",
    "classnames": "^2.2.1"
  },
  "engines": {
    "node": ">=8.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/runtime": "^7.22.15",
    "@rc-component/father-plugin": "^1.0.0",
    "@testing-library/react": "14.0.0",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^26.0.0",
    "@types/luxon": "^3.2.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "copyfiles": "^2.4.1",
    "coveralls": "^3.0.6",
    "cross-env": "^7.0.2",
    "date-fns": "2.x",
    "dayjs": "^1.11.9",
    "dumi": "^2.1.15",
    "eslint": "~7.32.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-jest": "^26.8.2",
    "eslint-plugin-react-hooks": "^4.0.2",
    "eslint-plugin-unicorn": "^40.0.0",
    "father": "^4.0.0",
    "glob": "^7.2.0",
    "less": "^3.10.3",
    "luxon": "3.x",
    "mockdate": "^3.0.2",
    "moment": "^2.24.0",
    "np": "^7.1.0",
    "postcss": "^8.4.20",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.0.2",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.8.1",
    "rc-picker": "^2.7.6",
    "rc-test": "^7.0.9",
    "rc-util": "^5.37.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.57.1",
    "typescript": "^4.0.3"
  },
  "peerDependencies": {
    "date-fns": ">= 2.x",
    "dayjs": ">= 1.x",
    "luxon": ">= 3.x",
    "moment": ">= 2.x",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "peerDependenciesMeta": {
    "date-fns": {
      "optional": true
    },
    "dayjs": {
      "optional": true
    },
    "luxon": {
      "optional": true
    },
    "moment": {
      "optional": true
    }
  }
}
